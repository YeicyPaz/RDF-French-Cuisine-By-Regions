@prefix u: <http://www.jyu.fi/ontology#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>  .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

###Classes

u:Region rdf:type rdfs:Class .

u:SpecialiteCulinaire rdf:type rdfs:Class .

u:Plat rdfs:subClassOf u:SpecialiteCulinaire .
u:Dessert rdfs:subClassOf u:SpecialiteCulinaire .
u:ProduitDuTerroir rdfs:subClassOf u:SpecialiteCulinaire .

u:Recette rdf:type rdfs:Class .


u:BoissonAlcoolisee rdfs:subClassOf u:ProduitDuTerroir .
u:Fromage rdfs:subClassOf u:ProduitDuTerroir .
u:Charcuterie rdfs:subClassOf u:ProduitDuTerroir .
u:BoissonNonAlcoolisee rdfs:subClassOf u:ProduitDuTerroir .
u:ProduitDeLaMer rdfs:subClassOf u:ProduitDuTerroir .
u:Vin rdfs:subClassOf u:BoissonAlcoolisee .
u:Liqueur rdfs:subClassOf u:BoissonAlcoolisee .


###Propriete

u:estOriginaireDe rdf:type rdf:Property ;
    rdfs:domain u:SpecialiteCulinaire ;
    rdfs:range u:Region .

u:aRecette rdf:type rdf:Property ;
    rdfs:domain u:Dessert;
    rdfs:domain u:Plat;
    rdfs:range u:Recette .

u:aMethodeDePreparation rdf:type rdf:Property ;
    rdfs:domain u:Recette ;
    rdfs:range xsd:string .
u:aTempsDePreparation rdf:type rdf:Property ;
    rdfs:domain u:Recette ;
    rdfs:range xsd:integer .
u:aIngredient rdf:type rdf:Property ;
    rdfs:domain u:Recette ;
    rdfs:range xsd:string .
u:aDifficulte rdf:type rdf:Property ;
    rdfs:domain u:Recette ;
    rdfs:range xsd:string .
u:aTypeDeRegime rdf:type rdf:Property ;
    rdfs:domain u:Recette ;
    rdfs:range xsd:string .


###Instance

u:Bretagne rdf:type u:Region .

u:GaletteBretonne rdf:type u:Plat ;
    u:estOriginaireDe u:Bretagne ;
    u:aRecette u:RecetteGaletteBretonne .
u:RecetteGaletteBretonne rdf:type u:Recette ;
    u:aTempsDePreparation 30 ;
    u:aDifficulte "facile" ;
    u:aTypeDeRegime "vegetarien" ;
    u:aIngredient "farine de sarrasin" .


u:KouignAmann rdf:type u:Dessert ;
    u:estOriginaireDe u:Bretagne ;
    u:aRecette u:RecetteKouignAmann .
u:RecetteKouignAmann rdf:type u:Recette ;
    u:aMethodeDePreparation "Pâte levée feuilletée, alternance de couches de beurre et de sucre" ;
    u:aTempsDePreparation 90 ;
    u:aIngredient "farine" ;
    u:aIngredient "beurre salé" ;
    u:aIngredient "sucre" ;
    u:aIngredient "levure" ;
    u:aDifficulte "difficile" ;
    u:aTypeDeRegime "vegetarien" .